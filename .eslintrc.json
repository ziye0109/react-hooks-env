{
    "extends": ["airbnb-base"],
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "react/jsx-uses-vars": 1,
      "react/jsx-uses-react": 1,
      "no-tabs": 0,
      "class-methods-use-this": 0,
      "import/no-unresolved": 0,
      "indent": 0,
      "comma-dangle": 0,
      "import/prefer-default-export": 0
    },
    "plugins": [
      "react"
    ],
    "globals": {
      "document": true,
      "navigator": true,
      "window":true,
      "node":true
    },
    "settings": {
      "import/resolver": {
        "node": {
           "extensions": [".js",".jsx"]
        }
      }
    }
  }